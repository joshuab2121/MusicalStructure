<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.joshb.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".NowPlaying"
            android:label="@string/now_playing">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MusicLibrary"
            android:label="@string/music_library" />
        <activity
            android:name=".ArtistDetail"
            android:label="@string/artist_detail" />
        <activity
            android:name=".Playlist"
            android:label="@string/playlist" />
    </application>

</manifest>